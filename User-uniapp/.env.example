# 环境变量配置模板
# 复制此文件为 .env 并填入实际配置值
# 注意：.env 文件不应提交到版本控制系统

# 服务器配置
VITE_SERVER_HOST=your_server_host_or_ip
VITE_SERVER_PORT=your_server_port
VITE_TUNNEL_URL=your_tunnel_url_or_leave_empty
VITE_USE_TUNNEL=false

# 微信云托管配置
VITE_USE_CLOUD_CONTAINER=true
VITE_CLOUD_ENV=your_wechat_cloud_environment_id
VITE_CLOUD_SERVICE=your_wechat_cloud_service_name